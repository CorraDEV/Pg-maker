<!DOCTYPE html>
<html lang="en">
<%-include('head', title)%>
<body>
    <%pages.map(page =>{%>
        <a href="<%=page.url%>"><button><%=page.title%></button></a>
    <%})%>    
    <%if(races.length > 0){%>
        <ul>
            <%races.map(race =>{%>
                <li><a href="<%=`${title}/${race.id}`%>"><%=`${race.ra_name}`%></a></li>
            <%})%>                            
        </ul>
    <%}%>
    <a href="<%=title%>/new"><button>Add Race</button></a>
</body>
</html>